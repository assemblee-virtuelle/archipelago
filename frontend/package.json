    "@mui/styles": "^5.15.14",
    "@mui/x-tree-view": "6.0.0-beta.0",
    "@semapps/auth-provider": "1.1.3",
    "@semapps/date-components": "1.1.3",
    "@semapps/field-components": "1.1.3",
    "@semapps/geo-components": "1.1.3",
    "@semapps/input-components": "1.1.3",
    "@semapps/interop-components": "1.1.3",
    "@semapps/list-components": "1.1.3",
    "@semapps/markdown-components": "1.1.3",
    "@semapps/semantic-data-provider": "1.1.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.3.4",
    "date-fns": "~2.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "leaflet.markercluster": "^1.5.3",
    "mui-tree-select": "^1.0.0-beta.11",
    "ra-language-french": "^5.6.3",
    "react": "^18.3.1",
    "react-admin": "^5.6.3",
    "react-dom": "^18.3.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vite-plugin-checker": "^0.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "source-map-explorer": "^2.5.2",
    "yalc": "1.0.0-pre.53"
  },
  "scripts": {
    "dev": "vite --force",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "link-semapps-packages": "yalc link @semapps/activitypub-components @semapps/auth-provider @semapps/date-components @semapps/field-components @semapps/geo-components @semapps/input-components @semapps/list-components @semapps/markdown-components @semapps/semantic-data-provider",
    "unlink-semapps-packages": "yalc remove --all && rm -rf node_modules/@semapps && yarn install --force"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  }
}
